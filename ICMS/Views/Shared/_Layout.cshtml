    <style type="text/css">
        html {
            font-size: 15px !important;
        }

        body {
            font-family: 'myriad-pro', serif !important;
            font-weight: 400;
            line-height: 1.45;
            color: #333 !important;
        }

        div, p, small, code, span, button, input, textarea, label {
            font-family: 'myriad-pro', serif !important;
        }

        .fa {
            font: normal normal normal 14px/1 FontAwesome !important;
        }

        .e-icons {
            font-family: 'e-icons' !important;
        }

        p {
            margin-bottom: 1.3em;
        }

        h1, h2, h3, h4 {
            font-family: 'proxima-nova-extra-condensed', serif !important;
            margin: 1.414em 0 0.5em;
            font-weight: inherit !important;
            line-height: 1.2 !important;
        }

        h1 {
            margin-top: 0;
            font-size: 2.441em !important;
        }

        h2 {
            font-size: 1.953em !important;
        }

        h3 {
            font-size: 24px !important;
        }

        h4 {
            font-size: 1.25em !important;
        }

        small, .font_small {
            font-size: 0.8em !important;
            color: #767676 !important;
        }

        small, .font_small, code, label {
            color: #767676 !important;
        }

        textarea {
            color: black !important;
            font-size: 15px !important;
        }
    </style>

    <!-- inline spinner styles to be able to display spinner right away -->
    <style type="text/css">
        .loader_wrapper {
            height: 200px;
            width: 200px;
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            margin: auto;
        }
	     .loading-2 .spinner {
	         box-sizing: border-box;
        }
         .loading-2 .spinner {
	         display: inline-block;
	         animation-name: anim-spinner;
	         animation-duration: 0.8s;
	         animation-iteration-count: infinite;
	         animation-timing-function: linear;
        }
         .loading-2 .spinner .circle {
	         width: 150px;
	         height: 75px;
	         overflow: hidden;
	         margin-top: -1px;
        }
         .loading-2 .spinner .circle-inner {
	         transform: rotate(45deg);
	         border-radius: 50%;
	         border: 12px solid #06206e;
	         border-right: 12px solid transparent;
	         border-bottom: 12px solid transparent;
	         width: 100%;
	         height: 200%;
	         animation-name: anim-circle-1;
	         animation-duration: 0.8s;
	         animation-iteration-count: infinite;
	         animation-direction: alternate;
	         animation-timing-function: cubic-bezier(0.25, 0.1, 0.5, 1);
        }
         .loading-2 .spinner .circle-2 {
	         transform: rotate(180deg);
        }
         .loading-2 .spinner .circle-2 .circle-inner {
	         animation-name: anim-circle-2;
        }
         @@keyframes anim-circle-1 {
	         from {
		         transform: rotate(60deg);
	        }
	         to {
		         transform: rotate(205deg);
	        }
        }
         @@keyframes anim-circle-2 {
	         from {
		         transform: rotate(30deg);
	        }
	         to {
		         transform: rotate(-115deg);
	        }
        }
         @@keyframes anim-spinner {
	         from {
		         transform: rotate(0deg);
	        }
	         to {
		         transform: rotate(360deg);
	        }
        }
    </style>
    <link rel="stylesheet" href="scripts/libs/styles.css">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/x-icon" href="scripts/libs/assets/images/smu_favicon.png">

</head>
<body>
    @RenderBody()
    @RenderSection("scripts", required: false)
    <link href="https://cdn.syncfusion.com/ej2/material.css" rel="stylesheet" rel="stylesheet" />
</body>
</html>
